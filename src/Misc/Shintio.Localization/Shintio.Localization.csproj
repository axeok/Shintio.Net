<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netcoreapp2.2.8;net6.0;net8.0;net9.0</TargetFrameworks>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<LangVersion Condition="'$(TargetFramework)' == 'netcoreapp2.2.8'">9</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<PackageId>Shintio.Localization</PackageId>
		<Version>1.0.0</Version>
		<Authors>axeok; nortages</Authors>
		<Company>Shintio</Company>
		<Description>Localization</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/axeok/Shintio.Net</PackageProjectUrl>
		<RepositoryUrl>https://github.com/axeok/Shintio.Net.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>Localization Shintio</PackageTags>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	</PropertyGroup>

	<ItemGroup>
	  <ProjectReference Include="..\..\Shintio.Essentials\Shintio.Essentials.csproj" />
	</ItemGroup>

</Project>
