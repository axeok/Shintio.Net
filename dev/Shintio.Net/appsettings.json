{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Trace",
      "Microsoft.EntityFrameworkCore": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "UseUtcTimestamp": true,
        "TimestampFormat": "HH:mm:ss.fff "
      }
    }
  }
}